{
  "version": 2,
  "name": "ate-bit",
  "public": true,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "cleanUrls": true,
  "rewrites": [
    { "source": "/assets/:path*", "destination": "/assets/:path*" },
    { "source": "/style.css", "destination": "/style.css" },
    { "source": "/main.js", "destination": "/main.js" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
} 