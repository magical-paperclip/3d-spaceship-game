{
  "version": 2,
  "public": true,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "cleanUrls": true,
  "routes": [
    { "src": "^/assets/(.*)$", "dest": "/assets/$1" },
    { "src": "^/style\\.css$", "dest": "/style.css" },
    { "src": "^/main\\.js$", "dest": "/main.js" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
} 